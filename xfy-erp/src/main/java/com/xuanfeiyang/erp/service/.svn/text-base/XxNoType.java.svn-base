	 * 调拨单号
	 */
	DB("DB"),
	
	/**
	 * 入库单号
	 */
	QR("QR"),
	
	/**
	 * 出库单号
	 */
	QC("QC"),
	
	/**
	 * 请购单号
	 */
	CS("CS"),
	
	/**
	 * 采购单号
	 */
