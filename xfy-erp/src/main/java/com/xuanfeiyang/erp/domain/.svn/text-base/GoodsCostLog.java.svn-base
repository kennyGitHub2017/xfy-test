package com.xuanfeiyang.erp.domain;

import java.math.BigDecimal;
import java.util.Date;

public class GoodsCostLog {
	
	private Integer Id;
	private Date createdTime; //时间
	private BigDecimal Cost; //修改的采购价格
	private String operUser; //操作人
	private String note; //说明
	private String goodsSku; //产品sku
	private Integer costUpType; //价格修改记录
	private Integer customUpType;//卖家价格维护类型
	private String customCost;//卖家价格
	
	public Integer getId() {
		return Id;
	}
	public void setId(Integer id) {
		Id = id;
	}
	public Date getCreatedTime() {
		return createdTime;
	}
	public void setCreatedTime(Date createdTime) {
		this.createdTime = createdTime;
	}
	public BigDecimal getCost() {
		return Cost;
	}
	public void setCost(BigDecimal cost) {
		Cost = cost;
	}
	public String getOperUser() {
		return operUser;
	}
	public void setOperUser(String operUser) {
		this.operUser = operUser;
	}
	public String getNote() {
		return note;
	}
	public void setNote(String note) {
		this.note = note;
	}
	public String getGoodsSku() {
		return goodsSku;
	}
	public void setGoodsSku(String goodsSku) {
		this.goodsSku = goodsSku;
	}
	public Integer getCostUpType() {
		return costUpType;
	}
	public void setCostUpType(Integer costUpType) {
		this.costUpType = costUpType;
	}
	public Integer getCustomUpType() {
		return customUpType;
	}
	public void setCustomUpType(Integer customUpType) {
		this.customUpType = customUpType;
	}
	public String getCustomCost() {
		return customCost;
	}
	public void setCustomCost(String customCost) {
		this.customCost = customCost;
	}
	
	

}
